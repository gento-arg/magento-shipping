{
    "name": "gento-arg/module-shipping",
    "description": "Gento_Shipping magento 2 extension",
    "require": {
        "php": ">=7.2",
        "gento-arg/module-core": "*",
        "magento/framework": "*",
        "magento/module-backend": "*",
        "magento/module-config": "*",
        "magento/module-ui": "*"        
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Gento\\Shipping\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpmd/phpmd": "^2.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
